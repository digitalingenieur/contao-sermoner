	"type": "contao-module",
	"license":"LGPL-3.0+",
	"keywords": ["contao","sermon","podcast"],
	"authors":[
		{
			"name":"Samuel Heer",
			"email":"samuel.heer@diging.de"
		}
	],
	"support":{
		"issues":"https://github.com/digitalingenieur/sermoner/issues"
		},
    "require": {
    	"php":">=5.3",
    	"contao/core-bundle":"~3.2 || ~4.1",
        "contao-community-alliance/composer-plugin":"~2.4 || ~3.0"
